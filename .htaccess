RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-d 

RewriteCond %{REQUEST_FILENAME}\.php -f

RewriteRule ^article/([0-9]+)/([0-9a-zA-Z_-]+) article.php?id=$1&name=$2 [NC,L]

RewriteCond %{REQUEST_FILENAME} !-d 

RewriteCond %{REQUEST_FILENAME}\.php -f

RewriteRule ^profile/([0-9]+)/([0-9a-zA-Z_-]+) profile.php?id=$1&name=$2 [NC,L]
RewriteRule ^(.*)$ $1.html